	public static boolean anagram(String a,String b){
		if(a.length()!=b.length())return false;
		int length[] = new int[256];
		int num_unique_chars = 0;
		int num_completed = 0;
		char[] ch = a.toCharArray();
		for(char c:ch){
			if(length[c]==0)++num_unique_chars;
			++length[c];
		}
		for(int i=0;i<b.length();i++){
			int index = (int)b.charAt(i);
			if(length[index]==0)return false;
			--length[index];
			if(length[index]==0){
				++num_completed;
			}
		}
			if(num_completed==num_unique_chars){
				return true;
			}
			else
				return false;
	}
