	public static String duplicates(String s)
	{
		char[] str = s.toCharArray();
		if(str == null) return "";
		int len = str.length;
		if(len<2)return s;
		int tail = 1;
		for(int i=1;i<len;i++)
		{
			int j;
			for(j=0;j<tail;j++)
			{
				System.out.println(i+" "+j+" "+tail);
				if(str[i]==str[j])break;
			}
			if(j==tail)
			{
				
				str[tail] = str[i];
				++tail;
			}
		}
		str[tail] = 0;
//		System.out.println(str);
		String a =new String(str);
		return a;
	}

